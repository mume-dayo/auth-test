# Discord Bot Configuration (Bot実行時に必要)
DISCORD_BOT_TOKEN=YOUR_BOT_TOKEN_HERE
DISCORD_CLIENT_ID=YOUR_CLIENT_ID_HERE
DISCORD_CLIENT_SECRET=YOUR_CLIENT_SECRET_HERE

# Web Server URL (Render等のデプロイURL)
WEB_URL=https://your-app.onrender.com

# Web Server Configuration (Web server実行時に必要)
PORT=3000
REDIRECT_URI=https://your-app.onrender.com/callback

# MongoDB Configuration (データ永続化用)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database_name
